{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./",
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": "./",
      "paths": {
        "*": ["*"]
      },
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "noImplicitAny": true
    },
    "include": ["server.ts"],
    "exclude": ["node_modules"]
  }
  